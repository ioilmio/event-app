Create a new event


<%= form_for Event.new do |f| %>
  <%= f.label :title%>
  <%= f.text_field :title %>

  <%= f.label :description%>
  <%= f.text_field :description %>

  <%= f.label :date%>
  <%= f.date_field :date%>

  <%= f.label :location%>
  <%= f.text_field :location%>

  <%= f.submit %>
<% end %>
